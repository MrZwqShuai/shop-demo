
{
  "defaultSeverity": "error",
  "extends": ["tslint:latest", "tslint-react"],
  "jsRules": {},
  "rules": {
    "adjacent-overload-signatures": false,  
    "ban-comma-operator": false, 
    "ban-type": [false, ["object","User {} instead."],["string"]],
    "no-any": false,
    "no-empty-interface":false,
    "no-import-side-effect": [true, {"ignore-module": "(\\.html|\\.css|\\.scss)$"}],
    "no-inferrable-types":[false, "ignore-params", "ignore-properties"],
    "no-internal-module":false,
    "no-magic-numbers": [false,1,2,3], 
    "no-namespace": [ false,"allpw-declarations"],
    "no-non-null-assertion": false , 
    "no-parameter-reassignment": false, 
    "no-reference": false, 
    "no-unnecessary-type-assertion":false, 
    "no-var-requires":false, 
    "only-arrow-functions":[false,"allow-declarations","allow-named-functions"],
    "prefer-for-of":false, 
    "promise-function-async": false,
    "typedef": [false, "call-signature", "parameter", "member-variable-declaration"], 
    "unified-signatures":false, 
    "await-promise":false,  
    "quotemark":[true,"avoid-escape"],
    "semicolon":[false, "never", "ignore-interfaces"],
    "indent":[false, "tabs", 2],
    "typedef-whitespace": false,
    "whitespace": false,
    "member-access": false,
    "one-line": false,
    "import-blacklist":false,
    "ordered-imports": false,
    "trailing-comma": [false, { 
      "multiline": {
          "objects": "ignore",
          "arrays": "never",
          "functions": "never",
          "typeLiterals": "ignore"
      },
      "esSpecCompliant": false 
  }],
  "no-trailing-whitespace": false,
  "arrow-parens": false,
  "eofline": false,
  "no-submodule-imports": false
  },
  "rulesDirectory": []
}
